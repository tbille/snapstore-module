{
  "scripts": {
    "serve": "talisker.gunicorn canonicalwebteam.snapstore.app:create_app\\(\\) --reload --log-level debug --timeout 9999 --access-logfile - --error-logfile - --bind 0.0.0.0:${PORT}",
    "clean": "rm -rf node_modules yarn-error.log css static/css *.log *.sqlite _site/ build/ .jekyll-metadata .bundle",
    "watch": "watch -p 'static/sass/**/*.scss' -c 'yarn run build'",
    "build": "",
    "test": "flake8 --exclude '*env*,node_modules,bin,lib,include'"
  },
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "cssnano": "^3.10.0",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.1.0",
    "sass-lint": "^1.10.2",
    "watch-cli": "^0.2.2"
  }
}
